    "husky": "^4.2.5",
    "lint-staged": "^10.2.9",
    "node-sass": "^4.14.1",
    "postcss-calc": "^7.0.2",
    "postcss-copy": "^7.1.0",
    "postcss-css-variables": "^0.17.0",
